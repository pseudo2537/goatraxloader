            <!-- /ko -->
            <!-- ko ifnot: photo() -->
                <div class="photo none"></div>
            <!-- /ko -->
        <!-- /ko -->
        </a>
   
        <div class="infobox">
            <p class="name"><a data-bind="attr: {href: trackpipeUrl()}, text: name()"></a></p>
            <p class="actions">
                <a data-bind="attr: {href: 'https://bandcamp.com/profile?from=menubar&id=' + id}">profile</a> &middot;
